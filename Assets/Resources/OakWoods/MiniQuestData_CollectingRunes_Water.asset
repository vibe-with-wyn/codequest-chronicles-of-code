  questions:
  - questionText: Which declaration correctly creates a String variable to store
      a player's name?
    choices:
    - choiceText: String name = Roran;
      isCorrect: 0
    - choiceText: String name = "Roran";
      isCorrect: 1
    - choiceText: string name = "Roran";
